/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here:
    not(in = sel, out =nsel)
    and(a = sel, b = in, out = a)
    and(a = nsel, b = in, out = b)
}
